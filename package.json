{
  "name": "report_detector",
  "version": "0.0.1",
  "description": "Auto detecting acedamic report and send notification",
  "main": "dist/main.js",
  "bin": "dist/main.js",
  "scripts": {
    "typeCheck": "tsc --noEmit",
    "bundle": "node ./bundle.js",
    "pack": "pkg ."
  },
  "author": "Hexiang Qi <qihexiang@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.15.7",
    "pkg": "^5.5.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@types/node": "^18.7.16",
    "@types/selenium-webdriver": "^4.1.4",
    "commander": "^9.4.0",
    "esbuild-node-externals": "^1.5.0",
    "selenium-webdriver": "^4.4.0",
    "tesseract.js": "^3.0.2"
  },
  "pkg": {
    "scripts": "./dist/main.js",
    "assets": [
      "./README.md",
      "node_modules/",
      "node_modules/selenium-webdriver/",
      "./node_modules/seleinium-webdriver/**/*",
      "./node_modules/tesseract.js/",
      "./node_modules/tesseract.js/**/*"
    ],
    "targets": [
      "node16-win-x64", "node16-linux-x64", "node16-win-arm64", "node16-linux-arm64"
    ],
    "outputPath": "./releases"
  }
}
